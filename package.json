{
  "name": "fcc-exercise-tracker",
  "version": "0.1.0",
  "description": "A REST API project, part of Free Code Camp's curriculum",
  "type": "commonjs",
  "scripts": {
    "tsc": "tsc",
    "dev": "nodemon",
    "build": "tsc -p tsconfig.build.json && npm run copy:assets",
    "copy:assets": "cp -r src/views dist/ && cp -r public dist/",
    "start": "node -r tsconfig-paths/register dist/app.js"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "injection-js": "^2.5.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.10",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.25",
    "xss": "^1.0.15"
  },
  "repository": {
    "url": "https://github.com/freeCodeCamp/boilerplate-project-exercisetracker"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "express"
  ],
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/helmet": "^0.0.48",
    "@types/hpp": "^0.2.6",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.12",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  }
}
